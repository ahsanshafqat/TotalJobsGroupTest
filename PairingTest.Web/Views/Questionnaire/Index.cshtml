@model PairingTest.Web.Models.QuestionnaireViewModel

<html>
<head>
    <title>@Model.QuestionnaireTitle</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        <h2>Index</h2>


        int i = 0;

        foreach (var questionsText in Model.QuestionsText)
        {
            <div>

                @Html.Label(questionsText)
                @Html.TextBox("questionAnswer" + i)
            </div>
            i++;
        }

        <br />
        <input type="submit" value="submit" />
    }
</body>
</html>
